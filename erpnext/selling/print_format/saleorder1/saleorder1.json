 "html": "{%- macro add_header(page_num, max_pages, doc, letter_head, no_letterhead) -%}\n    {% if letter_head and not no_letterhead %}\n    <div class=\"col-md-8 letter-head\" style=\"float:left !important\">{{ letter_head }}</div>\n    <div class=\"col-md-4\"><table style=\"border:1px solid black\"><tr><td>Date</td><td>Invoice#</td></tr><tr><td>{{doc.transaction_date}}</td><td>{{doc.name}}</td></tr></table></div>\n\n    {% endif %}\n    \n    {% if max_pages > 1 %}\n    <p class=\"text-right\">{{ _(\"Page #{0} of {1}\").format(page_num, max_pages) }}</p>\n    {% endif %}\n{%- endmacro -%}\n{{ add_header(0,1,doc,letter_head, no_letterhead) }}\n\n\n<small>\n<div class=\"row\">        \n     <div class=\"col-xs-6\">            \n        <div class=\"row\">\n           <h4 class=\"col-xs-12 text-left\">Invoice to:</h4>\n        </div>\n\t<div class=\"row\">\n            <div class=\"col-xs-12 text-left\"><big><b>{{ doc.customer or '' }}</b></big> </div>\n        </div>\n        <div class=\"row\">\n             <div class=\"col-xs-12 \"><big>{{ doc.address_display or '' }}</big></div>\n        </div>                 \n    </div>\n\n    <div class=\"col-xs-6\">            \n        <div class=\"row\">\n           <h4 class=\"col-xs-12 text-left\">Ship to:</h4>\n        </div>\n\t\t<div class=\"row\">\n            <div class=\"col-xs-12 text-left\"><big></big> </div>\n        </div>\n        <div class=\"row\">\n            <div class=\"col-xs-12 \"><big>{{ doc.address_display or '' }}</big></div>\n        </div>                 \n    </div>\n</div>\n\n<br>\n\n<table class=\"table table-condensed table-hover table-bordered\">\n  <tbody>\n\t<tr>\n            <td colspan=\"2\" class=\"text-left\">TERMS: </td>\n            <td class=\"text-left\">Rep</td>\n            <td  colspan=\"3\" class=\"text-left\">P.O Bo. # </td>\n        </tr>\n        <tr>\n\t    <th class=\"text-right\">Quantity</th>\n            <th class=\"text-left\">Item</th>\n            <th class=\"text-right\">Unit price</th>\n            <th class=\"text-right\">Lbs/Box</th>\n            <th class=\"text-right\">Kgs/Box</th>\n            <th class=\"text-right\">Amount</th>\n        </tr>\n        {%- for row in doc.items -%}\n        <tr>\n            <td style=\"width: 7%;\">{{ row.qty }}</td>\n            <td style=\"width: 49%;\"><b>{{ row.item_code or '' }} </b><br> \n                 {{ row.description or '' }}  </td>\n            <td style=\"width: 15%; text-align: right;\">{{ row.get_formatted(\"rate\", doc) or ''}}</td>\n            <td style=\"width: 7%; text-align: right;\">{{row.weight_lbs}}</td>\n            <td style=\"width: 7%; text-align: right;\">{{row.weight_kgs}}</td>\n           <td style=\"width: 15%; text-align: right;\">{{\n                row.get_formatted(\"amount\", doc) or ''}}</td>\n        </tr>\n        {%- endfor -%}\n    </tbody>\n</table>\n\n<div class=\"row\">\n    <div class=\"col-xs-6 text-right\"></div>\n    <div class=\"col-xs-4 text-right\"> <big><b>{{ _(\"Sub Total\") }}</b></big></div>\n    <div class=\"col-xs-2 \">\n      <big> {{doc.total}} </big>\n    </div>\n</div>  \n\n<div class=\"row\">\n    <div class=\"col-xs-6 text-right\"></div>\n    <div class=\"col-xs-4 text-right\"> <big><b>{{ _(\"Taxes\") }}</b></big></div>\n    <div class=\"col-xs-2 \">\n      <big> {{doc.total_taxes_and_charges}} </big>\n    </div>\n</div>  \n\n<div class=\"row\">\n    <div class=\"col-xs-6 text-right\"></div>\n    <div class=\"col-xs-4 text-right\"> <big><b>{{ _(\"Delivery Charges\") }}</b></big></div>\n    <div class=\"col-xs-2 \">\n      <big>{{doc.delivery_charges}}</big>\n    </div>\n</div>  \n\n<div class=\"row\">\n    <div class=\"col-xs-6 text-right\"></div>\n    <div class=\"col-xs-4 text-right\"> <big><b>{{ _(\"Grand Total\") }}</b></big></div>\n    <div class=\"col-xs-2 \">\n      <big>{{doc.grand_total}} </big>\n    </div>\n</div>  \n\n<div class=\"row\">\n    <div class=\"col-xs-6 text-right\"></div>\n    <div class=\"col-xs-4 text-right\"> <big><b>{{ _(\"Payment\") }}</b></big></div>\n    <div class=\"col-xs-2 \">\n      <big>  </big>\n    </div>\n</div>  \n\n<div class=\"row\">\n    <div class=\"col-xs-6 text-right\"></div>\n    <div class=\"col-xs-4 text-right\"> <big><b>{{ _(\"Balance Due\") }}</b></big></div>\n    <div class=\"col-xs-2 \">\n      <big>  </big>\n    </div>\n</div>  \n\n<br>\n\n<div class=\"row\" style=\"margin-top:50%\">\n    <div class=\"col-xs-4 text-left\">6075 Kestrel Rd, Mississauga,<br/> ON L5T 1Y8, Canada</div>\n    <div class=\"col-xs-4 text-left\"> <big><b>{{ _(\"PH:\") }} </b></big>  1234567890 </div>\n    <div class=\"col-xs-4 text-right\">\n      <big>www.sundineproduce.ca</big>\n    </div> \n</div>  \n\n</small>", 
